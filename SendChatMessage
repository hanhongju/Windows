#定义Message内容
/script msg="anything u want to say";


#广播
/script SendChatMessage(msg,"CHANNEL",nil,1);SendChatMessage(msg,"CHANNEL",nil,2);SendChatMessage(msg,"CHANNEL",nil,3);SendChatMessage(msg,"CHANNEL",nil,4);SendChatMessage(msg,"YELL");



